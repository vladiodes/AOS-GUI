{
    "PlpMain": {
        "Project": "iros",
        "Name": "detect_board_state",
        "Type": "Glue",
        "Version": 1.0
    },
    "GlueFramework": "ROS",
    "ModuleActivation": {
        "RosService": {
            "ImportCode": [
                {
                    "From": "panda_demo.srv",
                    "Import": [
                        "*"
                    ]
                }
            ],
            "ServicePath": "/observe_tic_tac_toe_board",
            "ServiceName": "detect_new_state",
            "ServiceParameters": []
        }
    },
    "LocalVariablesInitialization": [
        {
            "LocalVariableName": "obsr",
            "FromROSServiceResponse": true,
            "VariableType":"string",
            "AssignmentCode": ["obsr = __input.state"]
        }
    ]
}