<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<?import javafx.scene.text.Font?>
<VBox spacing="10" xmlns="http://javafx.com/javafx"
      xmlns:fx="http://javafx.com/fxml"
      fx:controller="frontend.finalproject.Controllers.SubControllers.AddSDFileSkillParamController"
      prefHeight="550.0" prefWidth="600.0"
      stylesheets="@../../style.css">
    <Pane prefHeight="80" style="-fx-background-color: #2D75E8;" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
        <children>
            <Label fx:id="titleLBL" layoutX="73.0" layoutY="30.0" text="Add SD file skill parameters" textFill="WHITE">
                <font>
                    <Font size="28.0" />
                </font>
            </Label>
        </children>
    </Pane>

    <VBox styleClass="SubMenuVBox" spacing="10">
        <HBox>
            <Label text="InputLocalVariable" styleClass="TextFieldLabel"/>
            <TextField fx:id="InputLocalVarTXT" styleClass="TextFieldForm"/>
        </HBox>
        <HBox>
            <Label text="FromGlobalVariable" styleClass="TextFieldLabel"/>
            <TextField fx:id="FromGlobalVarTXT" styleClass="TextFieldForm"/>
        </HBox>
        <HBox styleClass="TreeBoxWrapper"><Button onAction="#InsertClick" fx:id="insertBTN" styleClass="formBtn" text="Insert initialization"/></HBox>
    </VBox>

</VBox>
